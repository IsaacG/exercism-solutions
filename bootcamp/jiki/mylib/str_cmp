function my#str_cmp with a, b do
  if a == b do
    return 0
  end
  set len_a to my#length(a)
  set len_b to my#length(b)
  if len_a < len_b do
    set len to len_a
  else do
    set len to len_b
  end
  set idx to 1
  repeat len times do
    if a[idx] == b[idx] do
      change idx to idx + 1
    else do
      break
    end
  end
  set order to "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
  for each letter in order do
    if a[idx] == letter do
      return -1
    else if b[idx] == letter do
      return 1
    end
  end
end
